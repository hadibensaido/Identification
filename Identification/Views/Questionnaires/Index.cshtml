@model IEnumerable<MetierIdentification.Models.Questionnaire>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeQuestionnaire.libelleType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.libelleQuestionnaire)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.TypeQuestionnaire.libelleType, "Details", "Reponses", new { id = item.idQuestionnaire }, null)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.libelleQuestionnaire)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.idQuestionnaire }) |
                @Html.ActionLink("Details", "index", "Questions", new { id = item.idQuestionnaire }, null) |
                @Html.ActionLink("Delete", "Delete", new { id = item.idQuestionnaire })
            </td>
        </tr>
    }

</table>
